{
  "name": "ibrows/boxalino-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony2 Bundle to export and search entities using the boxalino web services",
  "keywords": ["Ibrows", "Boxalino", "Symfony2", "Bundle"],
  "homepage": "http://www.ibrows.ch",
  "license": "MIT",
  "authors": [
    {
      "name": "iBROWS",
      "homepage": "https://github.com/ibrows"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/symfony": "2.8.*",
    "doctrine/orm": "~2.2,>=2.2.3,<2.5",
    "doctrine/dbal": "<2.5",
    "doctrine/doctrine-bundle": "~1.4",
    "ychadwick/p13n-client-php": "dev-master",
    "stof/doctrine-extensions-bundle": "^1.2"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "v2.2.0",
    "phpunit/phpunit": "^5.2"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ychadwick/p13n-client-php.git"
    }
  ],
  "autoload": {
    "psr-4": { "Ibrows\\BoxalinoBundle\\": "" }
  }
}
